{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts",
    "src/app/**/*",
    "src/environments/**/*.ts"
  ],
  "exclude": [
    "src/main.server.ts",
    "server.ts",
    "src/server.ts",
    "src/app/app.config.server.ts",
    "server.ngtypecheck.ts",
    "src/main.server.ngtypecheck.ts"
  ]
}
